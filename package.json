{
   "name": "flipted",
   "version": "1.0.0",
   "description": "LMS for K-12, Senior Capstone Project.",
   "main": "apollo-server.ts",
   "scripts": {
      "generate": "graphql-codegen --config codegen.yml",
      "data:targets": "node data/targets.js",
      "data:objectives": "node data/objectives.js",
      "data:courses": "node data/courses.js",
      "data:taskInfos": "node data/taskInfos.js",
      "format": "prettier --write .",
      "test": "NODE_ENV=development jest",
      "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
      "build": "NODE_ENV=development sls package",
      "deploy": "NODE_ENV=development sls deploy",
      "debug": "concurrently \"graphql-codegen --config codegen.yml --watch\" \"SLS_DEBUG=* NODE_ENV=development node --inspect node_modules/.bin/sls offline -s dev\""
   },
   "author": "Matt Cuento",
   "license": "ISC",
   "dependencies": {
      "@aws-sdk/client-dynamodb": "^3.1.0",
      "@aws-sdk/util-dynamodb": "^3.1.0",
      "@graphql-tools/load-files": "^6.3.0",
      "@graphql-tools/merge": "^6.2.10",
      "@southlane/cognito-jwt-verifier": "^0.1.7",
      "@types/graphql-iso-date": "^3.4.0",
      "apollo-server": "^2.19.1",
      "apollo-server-lambda": "^2.21.0",
      "concurrently": "^6.0.2",
      "csv-parser": "^3.0.0",
      "graphql": "^15.4.0",
      "graphql-iso-date": "^3.6.1",
      "graphql-type-json": "^0.3.2",
      "serverless": "^2.32.1",
      "uid": "^2.0.0"
   },
   "devDependencies": {
      "@graphql-codegen/cli": "1.21.4",
      "@graphql-codegen/introspection": "1.18.2",
      "@graphql-codegen/typescript": "^1.22.0",
      "@types/chai": "^4.2.15",
      "@types/jest": "^26.0.22",
      "chai": "^4.3.1",
      "eslint": "^7.23.0",
      "jest": "^26.6.3",
      "mocha": "^8.3.0",
      "prettier": "2.2.1",
      "serverless-dotenv-plugin": "^3.3.0",
      "serverless-dynamodb-client": "0.0.2",
      "serverless-dynamodb-local": "^0.2.39",
      "serverless-offline": "^6.8.0",
      "serverless-webpack": "^5.3.5",
      "superagent": "^6.1.0",
      "ts-jest": "^26.5.4",
      "ts-loader": "^8.0.17",
      "ts-node": "^9.1.1",
      "typescript": "^4.2.3",
      "typings": "^2.1.1",
      "webpack": "^5.22.0",
      "webpack-node-externals": "^2.5.2"
   }
}
